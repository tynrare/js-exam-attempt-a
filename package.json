{
  "name": "webpack_template",
  "version": "1.0.0",
  "description": "Default webpack template for HTML projects",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "src": "src",
    "res": "res"
  },
  "scripts": {
    "unit-test": "karma start karma.config.js --single-run --mode=development",
    "unit-test-watch": "webpack-dev-server --open --env.test=unit --env.chunks=true --mode=development",
    "manual-test-watch": "webpack-dev-server --open --env.test=manual --env.chunks=true --mode=development",
    "manual-test-build": "webpack --config webpack.config.js --env.test=manual --env.chunks=true --mode=development",
    "build-dev": "webpack --config webpack.config.js --mode=development",
    "build-prod": "webpack --config webpack.config.js --mode=production",
    "watch": "webpack-dev-server --open --mode=development",
    "docs": "jsdoc ./src ./README.md -c .jsdoc.json --pedantic -r -d dist/reports/docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "better-docs": "^1.4.7",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jsdoc": "^15.12.2",
    "html-webpack-plugin": "^3.2.0",
    "inline-source-map": "^0.6.2",
    "jsdoc": "^3.6.3",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.2.2",
    "mocha-loader": "^3.0.0",
    "puppeteer": "^2.1.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "backbone": "^1.4.0",
    "backbone.localstorage": "^2.0.2",
    "handlebars": "^4.7.3",
    "jquery": "^3.5.0"
  }
}
